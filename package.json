{
  "name": "Palettable",
  "version": "1.0.0",
  "description": "Generate color palettes using the knowledge of millions of designers.",
  "author": "Alec Ortega",
  "repository": "git@github.com:alecortega/palettable.git",
  "scripts": {
    "deploy-web-client": "git subtree push --prefix=client heroku-web-client master",
    "deploy-back-end": "git subtree push --prefix=client heroku-back-end master",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node start-server.js",
    "client": "node start-client.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/*.js": [
      "prettier --single-quote --trailing-comma --write",
      "git add"
    ]
  },
  "license": "ISC",
  "dependencies": {
    "babel-eslint": "^7.2.3",
    "concurrently": "^3.5.0",
    "eslint": "^4.1.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "prettier": "^1.5.3",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.0.3"
  }
}
